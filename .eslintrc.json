{
    "env": {
        "node": true,
        "es6": true
    },
    "extends": [
        "airbnb-base"
    ],
    "rules": {
        "indent": [
            2,
            "tab"
        ],
        "no-unused-vars": "off",
        "new-cap": "off",
        "no-tabs": 0,
        "func-names": "off",
        "eol-last": "off",
        "no-unused-expressions": "off",
        "no-multiple-empty-lines":"off",
        "no-param-reassign": 0,
        "import/no-dynamic-require": 0
    }
}